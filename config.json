{
  "name": "default_config",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 16000,
    "spectrogram": {
      "type": "MelSpectrogram",
      "args": {
      }
    },
    "log_spec": true
  },
  "augmentations": {
    "wave": [
    ],
    "spectrogram": [
    ]
  },
  "arch": {
    "type": "RawNet",
    "args": {
      "conv_channels": [[20, 20], [128, 128]]
    }
  },
  "data": {
    "train": {
      "batch_size": 64,
      "num_workers": 0,
      "datasets": [
        {
          "type": "SpoofDataset",
          "args": {
            "part": "train",
            "data_dir": "/kaggle/input/asvpoof-2019-dataset/LA/LA"
          }
        }
      ]
    },
    "val": {
      "batch_size": 64,
      "num_workers": 0,
      "datasets": [
        {
          "type": "SpoofDataset",
          "args": {
            "part": "eval",
            "data_dir": "/kaggle/input/asvpoof-2019-dataset/LA/LA"
          }
        }
      ]
    }
  },
  "optimizer": {
    "type": "Adam",
    "args": {
      "lr": 1e-4,
      "weight_decay": 1e-4,
      "amsgrad": true
    }
  },
  "loss": {
    "type": "CELoss",
    "args": {}
  },
  "metrics": [
    {
      "type": "EERMetric",
      "args": {
        "name": "EER"
      }
    }
  ],
  "trainer": {
    "epochs": 40,
    "save_dir": "saved/",
    "save_period": 10,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 100,
    "visualize": "wandb",
    "wandb_project": "spoof_project",
    "len_epoch": 1000,
    "grad_norm_clip": 100
  }
}